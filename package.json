  "license": "SEE LICENSE IN README.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:vlocityinc/newport-design-system.git"
  },
  "scripts": {
    "build": "gulp build -v",
    "build:development": "lwc-services build",
    "build-prod": "gulp build",
    "build-storybook": "build-storybook -c .storybook -s . -o .out",
    "dist": "node scripts/dist.js",
    "gulp": "gulp",
    "lint": "gulp lint",
    "start": "npm run build && npm run storybook",
    "precommit": "lint-staged",
    "storybook": "start-storybook -s ./ -p 6006",
    "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
    "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\""
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "**/*.{css,html,js,json,md,ts,yaml,yml}": [
      "prettier --write"
    ],
    "./src/**/*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "lwc"
  ],
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "comment-parser": "^0.5.4",
    "gulp-clean-css": "^4.2.0",
    "highlight.js": "^9.15.6",
    "module": "^1.2.5",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.22.0"
  },
  "lwc": {
    "modules": [
      {
        "dir": "src/modules"
      }
    ]
  }
}
